@{
    Layout = null;
}
@model RISTExamOnlineProject.Models.db.vewOperatorAlls
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="keywords" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--Meta Responsive tag-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login</title>
    @*<script src="~/lib/font-awesome/js/a4c93cd695.js" crossorigin="anonymous"></script>*@
    @*<script src="https://kit.fontawesome.com/a4c93cd695.js" crossorigin="anonymous"></script>*@
    @*<link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />*@
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/fontawesome.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/solid.css" rel="stylesheet" />
    <link href="~/lib/Custom/stylecustom.css" rel="stylesheet" />
    <link href="~/lib/Custom/logincss.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
</head>
<body>

 
        <section>
            <div class="container">
                <div class="container-login100">

                    <div class="wrap-login100 shadow-lg">
                        <div class="login100-pic js-tilt" data-tilt>
                            <img src="~/lib/img/img-01.png" alt="IMG">
                        </div>
                       
                        <form class="login100-form" asp-action="login" method="post" asp-route-returnUrl="@ViewData["ReturnUrl"]" asp-antiforgery="false">
                           
                            <span class="login100-form-title">
                                Member Login
                            </span>

                            <div class="wrap-input100">

                                @Html.TextBoxFor(m => m.OperatorID, new { @class = "input100", placeholder = "Operator No." })


                                <span class="focus-input100"></span>

                                <span class="symbol-input100">
                                    <i class="fa fa-user" aria-hidden="true"></i>
                                </span>


                            </div>

                            <div class="wrap-input100 text-sm-center error-font">


                                @Html.ValidationMessageFor(m => m.OperatorID, "", new { @class = "text-danger" })


                            </div>

                            <div class="wrap-input100">

                                @Html.PasswordFor(m => m.Password, new { @class = "input100", placeholder = "Password" })
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="fa fa-lock" aria-hidden="true"></i>
                                </span>

                            </div>

                            <div class="wrap-input100 text-center text-sm-center error-font">
                                @Html.ValidationMessageFor(m => m.Password, "", new { @class = "text-danger" })
                            </div>

                            <div class="container-login100-form-btn">
                                <button class="login100-form-btn" type="submit">
                                    Login
                                </button>
                            </div>

                            <div class="wrap-input100 text-center text-sm-center error-font p-t-12">
                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                            </div>

                            <div class="text-center p-t-12">
                                <span class="txt1">
                                    Forgot
                                </span>
                                <a class="txt2" href="#">
                                    Username / Password?
                                </a>
                            </div>


                        </form>
                    </div>
                </div>
            </div>
        </section>
    


    <!--FOR IE9-->
    <script src="~/lib/html5shiv/html5shiv.js"></script>
    <script src="~/lib/respond.js/respond.js"></script>



</body>
</html>


